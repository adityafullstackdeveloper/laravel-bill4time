{
    "name": "adityafullstackdeveloper/laravel-bill4time",
    "description": "This is package for bill4time api integration",
    "keywords": [
        "adityafullstackdeveloper",
        "laravel",
        "laravel-bill4time"
    ],
    "homepage": "https://github.com/adityafullstackdeveloper/laravel-bill4time",
    "license": "MIT",
    "authors": [
        {
            "name": "Aditya Kumar",
            "email": "contact@adityadev.in",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Adityafullstackdeveloper\\LaravelBill4time\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adityafullstackdeveloper\\LaravelBill4time\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adityafullstackdeveloper\\LaravelBill4time\\LaravelBill4timeServiceProvider"
            ],
            "aliases": {
                "LaravelBill4time": "Adityafullstackdeveloper\\LaravelBill4time\\Facades\\LaravelBill4time"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
